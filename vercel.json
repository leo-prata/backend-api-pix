{ 
    "version": 2, 
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Acess-Control-Allow-Origin": "*"
            }
        }
    ]
}